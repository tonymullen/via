{
  "name": "meanstack_via_game",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Leonardo Maglio  @leota",
  "description": "MEAN stack boilerplate with authentication, 3D, and socket interaction.",
  "angular-cli": {},
  "engines": {
    "node": ">=6.11.4",
    "npm": ">=3.10.10"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.1",
    "@angular/cdk": "^6.4.2",
    "@angular/common": "^6.1.1",
